{
  "name": "No Seen",
  "description": "Blocks \"seen\" marks on Facebook Messenger and Instagram Stories.",
  "version": "1.3",
  "manifest_version": 2,
  "background": {
    "scripts": ["index.js"]
  },
  "options_ui": {
    "page": "preferences.html"
  },
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "*://*.facebook.com/*",
    "*://*.instagram.com/*"
  ],
  "developer": {
    "name": "Di√©ssica Gurskas",
    "url": "https://diessi.ca"
  },
  "applications": {
    "gecko": {
      "id": "no-seen@no.seen"
    }
  }
}
